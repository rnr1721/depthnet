APP_NAME=DepthNet
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

VITE_HOST=0.0.0.0
VITE_PORT=5173
VITE_DOCKER_ENV=true

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=depthnet
DB_USERNAME=depthnet
DB_PASSWORD=secret

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

DB_QUEUE_RETRY_AFTER=300

# Mock Engine
AI_MOCK_ENABLED=true
AI_MOCK_DEFAULT=true
AI_MOCK_DELAY=2
AI_MOCK_LANGUAGE=ru
AI_MOCK_SCENARIO_MODE=random
AI_MOCK_USER_INTERACTION=true
AI_MOCK_COMMAND_SIMULATION=true
AI_MOCK_DOPAMINE_RESPONSE=true
AI_MOCK_MAX_LENGTH=1000

# OpenAI Engine
AI_OPENAI_ENABLED=true
AI_OPENAI_DEFAULT=false
OPENAI_API_KEY=
OPENAI_API_URL=https://api.openai.com/v1/chat/completions
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=0.7
OPENAI_TOP_P=0.8
OPENAI_FREQUENCY_PENALTY=0.0
OPENAI_PRESENCE_PENALTY=0.0

# Claude Engine
AI_CLAUDE_ENABLED=true
AI_CLAUDE_DEFAULT=false
CLAUDE_API_KEY=
CLAUDE_API_URL=https://api.anthropic.com/v1/messages
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=4096
CLAUDE_TEMPERATURE=0.5
CLAUDE_TOP_P=0.7

# Local Engine
AI_LOCAL_ENABLED=true
AI_LOCAL_DEFAULT=false
AI_LOCAL_SERVER_URL=http://localhost:11434
AI_LOCAL_SERVER_TYPE=ollama
AI_LOCAL_MODEL=llama3
AI_LOCAL_MODEL_FAMILY=llama
AI_LOCAL_TEMPERATURE=0.7
AI_LOCAL_MAX_TOKENS=2048
AI_LOCAL_TOP_P=0.9
AI_LOCAL_TOP_K=40
AI_LOCAL_REPEAT_PENALTY=1.1
AI_LOCAL_TIMEOUT=60
AI_LOCAL_CLEANUP=true

# ==========================================
# AI PLUGINS CONFIGURATION
# ==========================================

# Global plugin settings
AI_EXECUTION_USER=
AI_PLUGIN_TIMEOUT=120
AI_PLUGIN_LOGGING=true
AI_PLUGIN_LOG_LEVEL=info
AI_PLUGIN_STATISTICS=true
AI_PLUGIN_HEALTH_MONITORING=true
AI_PLUGIN_HEALTH_INTERVAL=30

# Shell plugin specific
AI_SHELL_WORKING_DIR=/shared/httpd

# Security settings
AI_PLUGIN_REQUIRE_SUDO=true

# Performance settings
AI_PLUGIN_CONFIG_CACHE=true
AI_PLUGIN_OUTPUT_CACHE=false
AI_PLUGIN_MAX_CONCURRENT=3

# Debug settings (development only)
AI_PLUGIN_DEBUG=false
AI_PLUGIN_DEBUG_IO=false
AI_PLUGIN_TIMING=false
AI_PLUGIN_TEST_ON_BOOT=false
AI_PLUGIN_MOCK=false
AI_PLUGIN_MOCK_TESTS=true
