# Application Configuration
APP_NAME=DepthNet
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost:8000

# Database Configuration  
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=depthnet
DB_USERNAME=depthnet
DB_PASSWORD=secret
DB_ROOT_PASSWORD=your_secure_root_password

# CORS Settings for Vite
# Comma-separated list of allowed origins for external access
# Example: VITE_CORS_ORIGIN=http://192.168.1.100:8000,http://yourserver.com:8000
VITE_CORS_ORIGIN=

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# Sandbox configuration
# Set to 'sandbox' to enable AI code execution environments
# Leave empty or remove for lightweight mode (faster startup, less resources)
COMPOSE_PROFILES=

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# Session & Cache
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# CORS Configuration
# Maximum age for CORS preflight cache (in seconds)
CORS_MAX_AGE=86400

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

DB_QUEUE_RETRY_AFTER=300

# Sandbox Manager Configuration
SANDBOX_MANAGER_CONTAINER=depthnet-sandbox-manager-1
SANDBOX_MANAGER_SCRIPT_PATH=/sandbox-manager/scripts/manager.sh
SANDBOX_PREFIX=depthnet-sandbox
SANDBOX_NETWORK=depthnet_depthnet
SANDBOX_DEBUG=false

# Sandbox Configuration
SANDBOX_DEFAULT_TYPE=ubuntu-full
SANDBOX_MEMORY=512m
SANDBOX_CPUS=1.0
SANDBOX_TMPFS_SIZE=200m
SANDBOX_SECURITY_MODE=permissive
SANDBOX_DROP_CAPS=false
SANDBOX_DEFAULT_TIMEOUT=60
SANDBOX_DEFAULT_USER=sandbox-user
SANDBOX_DEFAULT_SHELL=bash
SANDBOX_READONLY_ROOT=false
SANDBOX_ENABLE_PRIVILEGED=false
SANDBOX_ENABLE_SYS_ADMIN=false

# Mock Engine
AI_MOCK_ENABLED=true
AI_MOCK_DEFAULT=true
AI_MOCK_DELAY=2
AI_MOCK_LANGUAGE=ru
AI_MOCK_SCENARIO_MODE=random
AI_MOCK_USER_INTERACTION=true
AI_MOCK_COMMAND_SIMULATION=true
AI_MOCK_DOPAMINE_RESPONSE=true
AI_MOCK_MAX_LENGTH=1000

# OpenAI Engine
AI_OPENAI_ENABLED=true
AI_OPENAI_DEFAULT=false
OPENAI_API_KEY=
OPENAI_API_URL=https://api.openai.com/v1/chat/completions
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=4096
OPENAI_TEMPERATURE=0.7
OPENAI_TOP_P=0.8
OPENAI_FREQUENCY_PENALTY=0.0
OPENAI_PRESENCE_PENALTY=0.0

# Claude Engine
AI_CLAUDE_ENABLED=true
AI_CLAUDE_DEFAULT=false
CLAUDE_API_KEY=
CLAUDE_API_URL=https://api.anthropic.com/v1/messages
CLAUDE_MODEL=claude-3-5-sonnet-20241022
CLAUDE_MAX_TOKENS=4096
CLAUDE_TEMPERATURE=0.5
CLAUDE_TOP_P=0.7

# Local Engine
AI_LOCAL_ENABLED=true
AI_LOCAL_DEFAULT=false
AI_LOCAL_SERVER_URL=http://localhost:11434
AI_LOCAL_SERVER_TYPE=ollama
AI_LOCAL_MODEL=llama3
AI_LOCAL_MODEL_FAMILY=llama
AI_LOCAL_TEMPERATURE=0.7
AI_LOCAL_MAX_TOKENS=2048
AI_LOCAL_TOP_P=0.9
AI_LOCAL_TOP_K=40
AI_LOCAL_REPEAT_PENALTY=1.1
AI_LOCAL_TIMEOUT=60
AI_LOCAL_CLEANUP=true

# Novita AI Configuration
AI_NOVITA_ENABLED=true
NOVITA_API_KEY=your_novita_api_key_here
NOVITA_MODEL=meta-llama/llama-3.1-8b-instruct
NOVITA_SERVER_URL=https://api.novita.ai/v3/openai/chat/completions
NOVITA_MODELS_ENDPOINT=https://api.novita.ai/v3/openai/models
NOVITA_TEMPERATURE=0.8
NOVITA_MAX_TOKENS=2048
NOVITA_TOP_P=0.9
NOVITA_FREQUENCY_PENALTY=0.0
NOVITA_PRESENCE_PENALTY=0.0
NOVITA_TIMEOUT=120
NOVITA_SYSTEM_PROMPT="You are a useful AI assistant."
NOVITA_LOG_USAGE=true

# Google Gemini AI Configuration
AI_GEMINI_ENABLED=true
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash
GEMINI_SERVER_URL=https://generativelanguage.googleapis.com/v1beta/chat/completions
GEMINI_MODELS_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models
GEMINI_TEMPERATURE=0.8
GEMINI_MAX_TOKENS=2048
GEMINI_TOP_P=0.9
GEMINI_TOP_K=40
GEMINI_TIMEOUT=120
GEMINI_SYSTEM_PROMPT="You are a useful AI assistant."
GEMINI_LOG_USAGE=true

# Gemini Safety Settings
GEMINI_SAFETY_ENABLED=true
GEMINI_SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
GEMINI_SAFETY_DANGEROUS_CONTENT=BLOCK_MEDIUM_AND_ABOVE

# Gemini Feature Flags
GEMINI_MULTIMODAL_ENABLED=true
GEMINI_THINKING_ENABLED=false
GEMINI_CONTEXT_CACHING=false
GEMINI_FUNCTION_CALLING=true
GEMINI_GROUNDING_ENABLED=false

# Fireworks AI Configuration
FIREWORKS_ENABLED=true
FIREWORKS_API_KEY=fw_your_api_key_here
FIREWORKS_MODEL=accounts/fireworks/models/llama-v3p1-8b-instruct
FIREWORKS_MAX_TOKENS=2048
FIREWORKS_TEMPERATURE=0.8
FIREWORKS_TOP_P=0.9
FIREWORKS_TIMEOUT=120
FIREWORKS_LOG_USAGE=true

# ==========================================
# AI PLUGINS CONFIGURATION
# ==========================================

# Global plugin settings
AI_EXECUTION_USER=
AI_PLUGIN_TIMEOUT=120
AI_PLUGIN_LOGGING=true
AI_PLUGIN_LOG_LEVEL=info
AI_PLUGIN_STATISTICS=true
AI_PLUGIN_HEALTH_MONITORING=true
AI_PLUGIN_HEALTH_INTERVAL=30

# Shell plugin specific
AI_SHELL_WORKING_DIR=/shared/httpd

# Security settings
AI_PLUGIN_REQUIRE_SUDO=true

# Performance settings
AI_PLUGIN_CONFIG_CACHE=true
AI_PLUGIN_OUTPUT_CACHE=false
AI_PLUGIN_MAX_CONCURRENT=3

# Debug settings (development only)
AI_PLUGIN_DEBUG=false
AI_PLUGIN_DEBUG_IO=false
AI_PLUGIN_TIMING=false
AI_PLUGIN_TEST_ON_BOOT=false
AI_PLUGIN_MOCK=false
AI_PLUGIN_MOCK_TESTS=true

# ==============================================
# AI ENVIRONMENT CONTEXT SETTINGS
# ==============================================

# Enable environment information in AI context
AI_ENVIRONMENT_ENABLED=true

# Include current working directory (security sensitive)
AI_ENVIRONMENT_INCLUDE_CWD=false

# Include PHP memory limit information
AI_ENVIRONMENT_INCLUDE_MEMORY=true

# Include detailed database connection info
AI_ENVIRONMENT_INCLUDE_DB_DETAILS=false

# Include system load information (Linux/Unix only)
AI_ENVIRONMENT_INCLUDE_LOAD=false

# Include disk space information
AI_ENVIRONMENT_INCLUDE_DISK=false
